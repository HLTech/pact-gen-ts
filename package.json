        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.11",
        "@types/qs": "^6.9.15",
        "axios": "^0.21.1",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "semantic-release": "^17.4.2",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.6.3"
    },
    "peerDependencies": {
        "typescript": "~5.6.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HLTech/pact-gen-ts.git"
    },
    "homepage": "https://github.com/HLTech/pact-gen-ts",
