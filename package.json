        "@types/node": "^14.14.35",
        "@types/qs": "^6.9.6",
        "husky": "^4.3.8",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "semantic-release": "^17.4.2",
        "typescript": "^4.2.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HLTech/pact-gen-ts.git"
    },
    "homepage": "https://github.com/HLTech/pact-gen-ts",
    "bugs": {
        "url": "https://github.com/HLTech/pact-gen-ts/issues"
