{
    "name": "pact-gen-ts",
    "version": "0.6.0",
    "description": "Generating pact files from typescript definitions",
    "keywords": [
        "pacts",
        "contracts",
        "tests",
        "typescript"
    ],
    "main": "./lib/index.js",
